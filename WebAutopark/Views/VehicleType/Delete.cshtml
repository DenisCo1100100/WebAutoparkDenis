@model WebAutopark.BusinessLogic.Models.VehicleTypeModel

@{
    ViewData["Title"] = "Delete VehicleType";
}

<hr />
<h4 class="text-center my-1">DELETE VEHICLE TYPE</h4>
<hr />
<div class="form-group text-center">
    <dl class="dl-horizontal">
        <dt>Id</dt>
        <dd>
            @Model.VehicleTypeId
        </dd>

        <dt>Name</dt>
        <dd>
            @Model.Name
        </dd>
    </dl>

    <form asp-controller="VehicleType" asp-action="Delete" method="post">
        <div class="form-group">
            <input type="submit" class="btn btn-outline-primary" value="Delete" />
        </div>
    </form>
</div>
<div class="text-center mt-3">
    <a asp-controller="VehicleType" asp-action="Index">Back to list</a>
</div>